---
import Entry from "@components/entry/Entry.astro";
import Footer from "@components/footer/Footer.astro";
import Header from "@components/header/Header.astro";
import UvodoCommunity from "@components/uvodo-community/UvodoCommunity.astro";
import UvodoFeedback from "@components/uvodo-feedback/UvodoFeedback.astro";
import UvodoInfo from "@components/uvodo-for/UvodoFor.astro";
import UvodoPaymentDeveloper from "@components/uvodo-payment-developer/UvodoPaymentDeveloper.astro";
import UvodoWork from "@components/uvodo-work/UvodoWork.astro";
import BaseHead from "../components/BaseHead.astro";

import { SITE_DESCRIPTION, SITE_TITLE } from "../config";
---

<!DOCTYPE html>
<html lang="en">
  <head>
    <BaseHead title={SITE_TITLE} description={SITE_DESCRIPTION} />
    <link
      rel="stylesheet"
      href="node_modules/keen-slider/keen-slider.min.css"
    />
  </head>
  <body>
    <div>
      <Header />
      <Entry />
      <UvodoInfo />
      <UvodoWork />
      <UvodoPaymentDeveloper />
      <UvodoFeedback />
      <UvodoCommunity />
      <Footer />
    </div>
    <style>
      .home {
        background: #f3f2fd;
      }
    </style>
  </body>
</html>
